<h1>Listing interviews</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Posting</th>
      <th>Interviewer</th>
      <th>Notes</th>
      <th>Contact method</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interviews.each do |interview| %>
      <tr>
        <td><%= interview.datetime %></td>
        <td><%= interview.posting %></td>
        <td><%= interview.interviewer %></td>
        <td><%= interview.notes %></td>
        <td><%= interview.contact_method %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
